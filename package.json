{
  "name": "obsidian-keyboard-analyzer",
  "version": "0.3.0",
  "description": "Analyse your keyboard shortcuts",
  "main": "main.js",
  "scripts": {
    "dev:setup": "node -e \"const fs=require('fs'),path=require('path');const cwd=process.cwd();const out=path.resolve(cwd,'..','obsidian-keyboard-analyzer-dev');fs.mkdirSync(out,{recursive:true});for (const f of ['manifest.json','versions.json']){const src=path.resolve(cwd,'build',f);if(fs.existsSync(src)) fs.copyFileSync(src,path.join(out,f));}\"",
    "dev:watch": "vite build --mode development --watch",
    "dev": "npm run dev:setup && npm run dev:watch",
    "build": "vite build --mode production",
    "release": "standard-version",
    "lint": "biome check .",
    "lint:fix": "biome check . --write"
  },
  "standard-version": {
    "t": ""
  },
  "keywords": [],
  "author": "Cogscides",
  "license": "MIT",
  "exports": {
    "./package.json": "./package.json"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@sveltejs/vite-plugin-svelte": "^5.1.1",
    "@types/node": "^24.2.0",
    "@unocss/extractor-svelte": "^66.4.1",
    "@unocss/vite": "^66.4.1",
    "obsidian": "^1.8.7",
    "obsidian-typings": "^3.15.0",
    "sanitize-html": "^2.17.0",
    "source-map-js": "^1.2.1",
    "svelte": "^5.37.3",
    "svelte-check": "^4.3.1",
    "svelte-preprocess": "^6.0.3",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "unocss": "^66.4.1",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "@tsconfig/svelte": "^5.0.4",
    "lucide-svelte": "^0.536.0",
    "obsidian-community-lib": "^2.0.2",
    "standard-version": "^9.5.0",
    "svelte-icons": "^2.1.0",
    "svelte-watch-resize": "^1.0.3"
  }
}
